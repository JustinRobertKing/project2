<% results.data.forEach((r) => { %>
  <div class="row">
    <div class="col s12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title teal-text text-accent-2" style="font-size: 1.5em;"><strong><%= r.name %></strong></span>
          <p><%= r.streetAddress%></p>
          <p><%= r.locality%>, <%= r.region%> <%= r.postalCode%></p>
        </div>
        <div class="card-action">
          <a href="/breweries/show">More Info</a>
        </div>
      </div>
    </div>
  </div>
<% }) %>